<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
namespace：命名空间，通常为"包名+SQL映射文件名"
-->
<mapper namespace="com.sp.dao.CodeDao">


    <!--根据id查询对应的证件信息-->
    <select id="getCodeById" resultType="Code">
        select * from t_code where id=#{id}
    </select>


</mapper>